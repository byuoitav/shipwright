{
  "oit-static-av-rooms": {
    "order": 0,
    "template": "oit-static-av-room",
    "settings": {
      "index": {
        "number_of_shards": "1"
      }
    },
    "mappings": {
      "_default_": {
        "_all": {
          "enabled": true,
          "norms": false
        },
        "dynamic_templates": [
          {
            "message_field": {
              "match": "message",
              "match_mapping_type": "string",
              "mapping": {
                "fielddata": {
                  "format": "disabled"
                },
                "fields": {
                  "raw": {
                    "ignore_above": 256,
                    "index": "not_analyzed",
                    "type": "string",
                    "doc_values": true
                  }
                },
                "index": "analyzed",
                "omit_norms": true,
                "type": "string"
              }
            }
          },
          {
            "string_fields": {
              "match": "*",
              "match_mapping_type": "string",
              "mapping": {
                "fielddata": {
                  "format": "disabled"
                },
                "fields": {
                  "raw": {
                    "ignore_above": 256,
                    "index": "not_analyzed",
                    "type": "string",
                    "doc_values": true
                  }
                },
                "index": "analyzed",
                "omit_norms": true,
                "type": "string"
              }
            }
          },
          {
            "double_fields": {
              "match": "*",
              "match_mapping_type": "double",
              "mapping": {
                "doc_values": true,
                "type": "double"
              }
            }
          },
          {
            "long_fields": {
              "match": "*",
              "match_mapping_type": "long",
              "mapping": {
                "doc_values": true,
                "type": "long"
              }
            }
          },
          {
            "date_fields": {
              "match": "*",
              "match_mapping_type": "date",
              "mapping": {
                "doc_values": true,
                "type": "date"
              }
            }
          }
        ],
        "properties": {
            "buildingID": {
                "type": "keyword"
            },
            "roomID": {
                "type": "keyword"
            },
            "notifications-suppressed": {
                "type": "boolean"
            },
            "alerting": {
                "type": "boolean"
            },
            "last-state-recieved": {
                "type": "date"
            },
            "last-heartbeat": {
                "type": "date"
            },
            "last-user-input": {
                "type": "date"
            },
            "power": {
                "type": "keyword"
            },
            "view-devices": {
                "type": "keyword"
            },
            "view-alerts": {
                "type": "keyword"
            },
            "enable-notifications": {
                "type": "keyword"
            },
            "suppress-notifications": {
                "type": "keyword"
            }
        }
    },
    "aliases": {}
  }
}
